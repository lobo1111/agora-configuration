<?xml version="1.0" encoding="UTF-8" standalone="yes" ?> 
<zpk>
    <id>$!zpk.getId()</id>
    #if($!zpk.getAccount())
        <description>$!zpk.getAccount().getNumber()</description>
    #end
    #if($!zpk.getPossession())
        <description>$!zpk.getPossession().getFullAddress()</description>
    #end
    #if($!zpk.getContractor())
        <description>$!zpk.getContractor().getName()</description>
    #end
    <number>$!zpk.getType().getKey()-$!zpk.getNumber()</number>
    <startCredit>$!zpk.getCurrentBalance().getStartCredit()</startCredit>
    <startDebit>$!zpk.getCurrentBalance().getStartDebit()</startDebit>
</zpk>