<?xml version="1.0" encoding="UTF-8" standalone="yes" ?> 
<owners>
    #foreach($row in $owners)
    <item>
        <id>$!row[0]</id>
        <name>$!row[1]</name>
        <number>$!row[2]</number>
        <type_label>$!row[3]</type_label>
        <type_key>$!row[4]</type_key>
    </item>
    #end
    <settings>
        <maxResults>$countRows</maxResults>
    </settings>
</owners>
