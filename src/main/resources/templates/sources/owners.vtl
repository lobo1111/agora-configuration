<?xml version="1.0" encoding="UTF-8" standalone="yes" ?> 
<payments>
    #foreach($row in $owners)
    <item>
        <id>$row[0]</id>
        <name>$row[1]</name>
        <number>$row[2]</number>
        <type>$row[3]</type>
    </item>
    #end
    <settings>
        <maxResults>$countRows</maxResults>
    </settings>
</payments>
