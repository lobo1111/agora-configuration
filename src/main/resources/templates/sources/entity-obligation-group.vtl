<?xml version="1.0" encoding="UTF-8"?>
<obligationGroups>
    <id>$!row.getId()</id>
    <name>$!row.getName()</name>
    <community>
        <id>$!obligation.getCommunity().getId()</id>
    </community>
    <obligations>
        #foreach($obligation in $row.getObligations())
        <obligation>
            <id>$!obligation.getId()</id>
            <name>$!obligation.getName()</name>
        </obligation>
        #end
    </obligations>
    <zpks>
        #foreach($zpk in $row.getZpks())
        <zpk>
            <id>$!zpk.getId()</id>
            <description>$!zpk.getDescription()</description>
            <number>$!zpk.getNumber()</number>
        </zpk>
        #end
    </zpks>
</obligationGroups>
