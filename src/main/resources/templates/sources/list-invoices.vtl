<?xml version="1.0" encoding="UTF-8" standalone="yes" ?> 
<entity name="invoices">
    <property name="maxResult" type="Integer">$countRows</property>
    <list name="items">
        #foreach($row in $items)
        <entity name="invoice">
            <property name="id" type="Integer">$row.getId()</property>
            <property name="number" type="String">$!row.getAttribute('NUMBER').getValue()</property>
            <property name="createdAt" type="String">$!row.getAttribute('CREATE_DATE').getValue()</property>
            <property name="contractor" type="String">$!row.getContractor().getName()</property>
            <property name="value" type="String">$!row.getAttirbute('VALUE').getValue()</property>
            <property name="payed" type="String">-$!row.getAttirbute('PAYED').getValue()-</property>
            <property name="accepted" type="String">-$!row.getAttirbute('ACCEPTED').getValue()-</property>
        </entity>
        #end
    </list>
</entity>