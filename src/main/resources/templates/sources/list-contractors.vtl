<?xml version="1.0" encoding="UTF-8" standalone="yes" ?> 
<entity name="contractors">
    <property name="maxResult" type="Integer">$countRows</property>
    <list name="items">
        #foreach($row in $contractors)
        <entity name="contractor">
            <property name="id" type="Integer">$row.getId()</property>
            <property name="name" type="String">$!row.getCompany().getName()</property>
        </entity>
        #end
    </list>
</entity>