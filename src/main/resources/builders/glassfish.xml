<project name="Scripting Container - glassfish" default="help" basedir=".">
    <description>Scripting Container - glassfish</description>

    <target name="gf-deploy" depends="init">
        <sun-appserv-deploy
             file="${earPath}"
             name="Container"
             force="true"
             precompilejsp="false"
             verify="false"
             upload="true"
             user="${gfAdminUser}"
             passwordfile="${gfPasswordFile}"
             host="localhost"
             port="${gfAdminPort}"
             target="${default-instance-name}"
             asinstalldir="${gfAsadminPath}" />
    </target>

</project>